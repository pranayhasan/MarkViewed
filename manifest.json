{
  "manifest_version": 2,

  "name": "Mark Viewed",
  "description": "This extension shows if the current page has been Viewed previously",
  "version": "1.0",

  "browser_action": {
    "default_icon": "viewed.png",
    "default_title": "Current URL Viewed History",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "history",
    "tabs",
    "background",
    "https://ajax.googleapis.com/"
  ]
}